#include <stdio.h>
#include <stdlib.h>
#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
int main()
{
    int n, m;
    int flag = 1;
    scanf("%d %d", &n, &m);
    int a[n][m];
    int max[n];
    int min[m];
    for (int i = 0; i < n; ++i)
    {
        for (int j = 0; j < m; ++j)
        {
            scanf("%d", &a[i][j]);
        }
    }
    for (int j = 0; j < m; ++j)
    {
        min[j] = 0;
    }
    for (int i = 0; i < n; ++i)
    {
        max[i] = 0;
    }
    for (int i = 0; i < n; ++i)
    {
        for (int j = 0; j < m; ++j)
        {
            if (a[i][j] > a[i][max[i]])
            {
                max[i] = j;
            }
            if (a[i][j] < a[min[j]][j])
            {
                min[j] = i;
            }
        }
    }
    bool ans = false;
    for (int i = 0; (!ans) && (i < n); ++i)
    {
        if (i == min[max[i]])
        {
            ans = true;
            printf("%d %d", i, max[i]);
        }
    }
    if (!ans)
        printf("%s", "none");
}//1.8
