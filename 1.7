#include <stdio.h>
#include <stdlib.h>
#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
int main()
{
    int n;
    scanf("%lld", &n);
    int x = 1;
    while (x * x < n)
        ++x;
    ++x;
    if (x > (n + 1))
        x = n + 1;
    int (*a) = (int(*))malloc((x) * sizeof(*a));
    for (int i = 0; i < x; ++i)
        a[i] = 0;
    a[1] = 1;
    for (int i = 2; i * i < x; ++i)
    {
        for (int j = i * i; j < x ; j += i)
            a[j] = 1;
    }
    int ans = 1;
    for (int i = 1; i < x; ++i)
    {
        if (a[i] == 0)
        {
            if (n % i == 0)
                ans = i;
            while (n % i == 0)
                n = n / i;
        }
    }
    if (n > ans)
        ans = n;
    printf("%lld", ans);
} //1.7
