#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
int main()
{
    int n, k;
    scanf("%d", &n);
    scanf("%d", &k);
    int a[100];
    for (int i = 0; i < n; ++i)
        scanf("%d", &a[i]);
    int csum = 0;
    int i = 0;
    for (; i < k; ++i)
        csum += a[i];
    int ans = csum;
    for (; i < n; ++i)
    {
        csum += a[i] - a[i - k];
        if (csum > ans)
            ans = csum;
    }
    printf("%d\n", ans);
} // 1.6
