#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int ispower(int n)
{
    return (n & (n - 1)) == 0;
}
void solve(int csum, int leftbord, int sz, int * a, int * ans)
{
    if (leftbord == sz - 1)
        return;
    if (ispower(csum))
        ++(*ans);
    for (int i = leftbord + 1; i < sz; ++i)
        solve(csum + a[i], i, sz, a, ans);
}
int main()
{
    int  n;
    scanf("%d", &n);
    int a[24];
    for (int i = 0; i < n; ++i) {
        scanf("%d", &a[i]);
    }
    int ans = 0;
    for (size_t i = 0; i < n; ++i)
        solve(a[i], i, n, a, &ans);
    printf("%d\n", ans);
//    for (int i = 1; i < 17; ++i)
//        printf("%d: %d\n", i, ispower(i));
} //2.2
